package Ruben.Tema2;

import java.util.Scanner;

public class Tema2Ejercicio5 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		int numero = 0;
		int mayor = 0;
		int cambio = 0;
		
		do {
			System.out.println("Pon un numero: ");
			numero = sc.nextInt();
			
			if (numero>mayor) {
			mayor=numero;
			
			
			}
		}
		while (numero!=0);
		
	}

}
